<%= form_for(@comedian) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :required => 'required' %>
  <%= f.label :thumbnail %>
  <%= f.text_field :thumbnail %>
  <%= f.label :age %>
  <%= f.number_field :age, :required => 'required' %>
  <%= f.label :city %>
  <%= f.text_field :city, :required => 'required' %>
  <%= f.submit ('Submit') %>
<% end %>
